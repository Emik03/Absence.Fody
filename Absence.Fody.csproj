<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <Description>Removes unused private/internal types during compile-time.</Description>
    </PropertyGroup>
    <ItemGroup>
        <WeaverFiles Remove="$(Absence)Absence.Fody.dll" />
        <Compile Remove="$(SharedPath)Emik.Morsels/*.cs" />
        <Content Remove="$(AssemblyName).ExternalAnnotations.xml" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Remove="Fody" />
        <PackageReference Remove="Asyncify" />
        <PackageReference Remove="Lazy.Fody" />
        <PackageReference Remove="AsyncFixer" />
        <PackageReference Remove="Equals.Fody" />
        <PackageReference Remove="InfoOf.Fody" />
        <PackageReference Remove="CsharpMacros" />
        <PackageReference Remove="Absence.Fody" />
        <PackageReference Remove="UnitGenerator" />
        <PackageReference Remove="InlineIL.Fody" />
        <PackageReference Remove="ToString.Fody" />
        <PackageReference Remove="Caseless.Fody" />
        <PackageReference Remove="NullGuard.Fody" />
        <PackageReference Remove="LocalsInit.Fody" />
        <PackageReference Remove="Substitute.Fody" />
        <PackageReference Remove="Virtuosity.Fody" />
        <PackageReference Remove="InlineMethod.Fody" />
        <PackageReference Remove="ConfigureAwait.Fody" />
        <PackageReference Remove="Scifa.CheckedExceptions" />
        <PackageReference Remove="JetBrainsAnnotations.Fody" />
        <PackageReference Include="FodyHelpers" Version="6.6.4" />
    </ItemGroup>
</Project>
